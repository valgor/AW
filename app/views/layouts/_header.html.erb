<nav class ="round">
  <ul>
    <li><%= link_to "Home" , root_path %></li>
    <% if user_signed_in? %>
        <li>Signed in as <%= current_user.email %>. Not you?
          <%= link_to "Sign_out", destroy_user_session_path, :method => :delete %></li>
        <li><%= link_to "Edit Account", edit_user_registration_path %></li>
    <% else %>
        <li>
          <%= link_to "Sign_up" , new_user_registration_path %> or
          <%= link_to "Sign_in" , new_user_session_path %></li>
    <% end %>
  </ul>
</nav>

